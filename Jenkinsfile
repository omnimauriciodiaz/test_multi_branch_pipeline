pipeline{
    agent any
    stages{
        stage('Stage 1'){
            steps{
                echo 'Step one from stage 1'
                echo 'Step two from stage 1'
            }
        }
        stage('Stage 2'){
            steps{
                echo 'Step one from stage 2'
                echo 'Step two from stage 2'
            }
        }
        stage('End stage'){
            steps{
                echo 'Close all'
                echo pwd
            }
        }
    }
}
